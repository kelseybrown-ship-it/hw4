<h1><%= @place["name"] %></h1>

<p>
  <a class="btn btn-outline-secondary" href="/entries/new?place_id=<%= @place["id"] %>" role="button">New Post</a>
 </p>


<% for entry in @entries %>
    <h3><%= entry["title"] %></h3>
    <h4><%= entry["occurred_on"] %></h4>
    <img class="img-fluid" src="<%= entry["image"] %>" class="w-full">
        <p><%= entry["description"] %></p>
<% end %>




<p>
  <a href="/places">Back to All Places</a>
</p>